<script type="text/javascript">
    $(function () {

        //Search Bar
        var getVenueIdFromCookie = "phx";
        var aolCustomLinkSearch = '';
        var searchLink = (aolCustomLinkSearch == null || aolCustomLinkSearch =='') ? 'http://us.wow.com/search?s_pt=boingo&q=' : aolCustomLinkSearch + '/search?s_pt=boingo&q=';

        $('form').each(function(){  

            $(this).find('#aolSearchBox').keypress(function(event){
                if(event.keyCode == 13){
                    window.open(searchLink + $('#aolSearchBox').val());
                    return false;
                }
            })

            $(this).find('#aolSearchButton').on('click', function(){
                //window.open(searchLink + $('#aolSearchBox').val());
                $('#aolSearchButton').attr({'href': searchLink + $('#aolSearchBox').val(), 'target': '_blank'});
            });
        });
                

        // format time (note: does handle time over 24 hours)
        function formatTime(timeInMilliseconds) {
            // convert milliseconds into date object in order to use date functions
            // start with epoch "January 1, 1970 - 00:00:00.000" and convert to milliseconds then add elapsed time
            var timeInDate = new Date(new Date(1970, 0, 1, 0, 0, 0, 0).getTime() + timeInMilliseconds);

            // pad with zeros
            var hours = ("0" + timeInDate.getHours()).slice(-2);
            var minutes = ("0" + timeInDate.getMinutes()).slice(-2);
            var seconds = ("0" + timeInDate.getSeconds()).slice(-2);

            // compose time displayed with hours conditionally shown
            return (hours > 0 ? hours + ":" : "") + minutes + ":" + seconds;
        }

        // update time remaining
        function updateSessionTimeoutRemaining(futureTimeoutInMilliseconds) {
            // calculate session timeout remaining from future timeout and current time
            // must calculate the current time each cycle since the web page javascript
            // might be suspended for certain situtations (eg. iphone screen lock)
            var sessionTimeoutRemainingInMilliseconds = Math.floor(futureTimeoutInMilliseconds - new Date().getTime());

            // call recursively with 1 second delay to update display
            if (sessionTimeoutRemainingInMilliseconds >= 0) {
                // update if greater or equal to 0
                $("#sessionTimeoutRemaining").html(formatTime(sessionTimeoutRemainingInMilliseconds));

                window.setTimeout(function () {
                    updateSessionTimeoutRemaining(futureTimeoutInMilliseconds);
                }, 1000);
            }
                // set timer to zero and end recursion
            else {
                $("#sessionTimeoutRemaining").html(formatTime(0));
            }
        }

        // get futureTimeout from cookie or from HotspotSite.xml:freeSessionTimeout
        function retrieveFutureTimeoutInMilliseconds() {
            var sessionTimeoutInSeconds = 1439.96668349333 * 60; // session timeout for venue in seconds

            // store time in future when session will expire
            var futureTimeoutInMilliseconds = new Date().getTime() + sessionTimeoutInSeconds * 1000; // add session timeout milliseconds

            return futureTimeoutInMilliseconds;
        }

        // update session timeout displayed
        updateSessionTimeoutRemaining(retrieveFutureTimeoutInMilliseconds());
    });
</script>


