<script>
    function roamerOnChange(currentOption) {
        var offsiteRoamerForm = document.getElementById("offsite_roamer");

        if (currentOption.title.length > 0) {
            offsiteRoamerForm.action = $(currentOption).attr("title");
            offsiteRoamerForm.HSPNAME.value = $(currentOption).data("roamer-hsp-name");
            offsiteRoamerForm.VNPNAME.value = $(currentOption).data("roamer-vnp-name");
            offsiteRoamerForm.LOCATIONNAME.value = $(currentOption).data("roamer-location-name");
            offsiteRoamerForm.submit();
        }
    }
    $(document).ready(function () {
        if ($('#roaming_partners_dropdown').val() != 'boingo') {
            $('.reset').hide();
        }

        $('#roaming_partners_dropdown').change(function () {
            var item = $(this);
            if (item.val() == 'boingo') {
                $('.reset').show();
            }
            else {
                $('.reset').hide();
            }
        });

        var login = $('.login-form input[type=submit]');
        var user = $('.login-form input[name=username]');
        var pwd = $('.login-form input[name=password]');

        $(login).on('click', function () {
            user.css('border', 'solid 1px #D52B1E').addClass('input-focus').attr('placeholder', 'Username Required');
            pwd.css('border', 'solid 1px #D52B1E').addClass('input-focus').attr('placeholder', 'Password Required');

            if (navigator.userAgent.indexOf("Safari") != -1) {
                if ((user.val().length < 1) || (pwd.val().length < 1)) {
                    user.css('border', 'solid 1px #D52B1E').addClass('input-focus').attr('placeholder', 'Username Required');
                    pwd.css('border', 'solid 1px #D52B1E').addClass('input-focus').attr('placeholder', 'Password Required');
                    return false;
                }
            }

            if (navigator.appName.indexOf("Internet Explorer") != -1) {
                if (document.documentMode < 10) {
                    if (user.val().length < 1) {
                        $('.wifiLogin input').css('border', 'solid 2px #D52B1E');
                        $('.wifiLogin input.blue.primary-font_heavy').css('border', 'none')
                        console.log(document.documentMode);
                        return false;
                    }
                }
            }
        });
    });

</script>

